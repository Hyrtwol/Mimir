{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Odin Debug Launch",
			"type": "cppvsdbg",
			"request": "launch",
			"preLaunchTask": "odin debug build",
			"program": "${workspaceFolder}/bin/${fileDirnameBasename}.exe",
			"args": [],
			"environment": [
				{
					"name": "config",
					"value": "Debug"
				}
			],
			"cwd": "${workspaceFolder}/bin",
			"console": "internalConsole",
			//"console": "externalTerminal",
			"stopAtEntry": false
		}
	]
}
