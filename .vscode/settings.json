		"unorm",
		"ushort",
		"viewports",
		"wndproc",
		"wolfenstein",
		"wparam",
		"wstring"
	],
	"markdownlint.config": {
		"no-hard-tabs": false,
		"line-length": false
	},
	"[markdown]": {
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
	},
	"omnisharp.autoStart": false,
	"typescript.tsc.autoDetect": "off",
	"npm.autoDetect": "off",
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/desktop.ini": true
	},
	"files.associations": {
		"*.lst": "plaintext",
		"*.fdp": "xml",
		"*.recipe": "msbuild",
		"*.csproj": "xml"
	},
	"workbench.editorAssociations": {
		"*.fsb": "hexEditor.hexedit",
		"*.fev": "hexEditor.hexedit",
		"*.res": "hexEditor.hexedit",
		"*.rom": "hexEditor.hexedit"
	},
	"cSpell.enableFiletypes": [
		"odin"
	],
}
