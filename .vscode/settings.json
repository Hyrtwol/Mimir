		"expectf",
		"flac",
		"fmod",
		"fmodex",
		"glsl",
		"hbitmap",
		"hgdiobj",
		"hinstance",
		"hlsl",
		"hwnd",
		"imag",
		"libfmodex",
		"lightwave",
		"linalg",
		"localizable",
		"lparam",
		"lresult",
		"lwid",
		"metaprogramming",
		"microarch",
		"microui",
		"mipmaps",
		"odinfmt",
